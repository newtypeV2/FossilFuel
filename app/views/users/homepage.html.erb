
  <h3>Matches:</h3>
  <div class="row">
    <% current_user.matches.each do |match| %>
      <div class="col">
          <%= render partial: "shared/avatar", locals: {object: match}%>
          <li><%= link_to match.name,user_path(match)  %></li>
      </div>
    <% end %>
  </div>
  
  <div class="row">
      <div class="col">
      <h3>Messages:</h3>
      <% current_user.receiver_relationships.each do |message| %>
      <div class="col">
      <h5>From: <%= message.sender.name %></h5>
      <p><%= message.content %></p>
      </div>
      <% end %>
    </div>
    <div class="col">
      </div>
    </div>

